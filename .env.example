
# Aplikasi
# APP_KEY dibuat dengan perintah: php artisan key:generate
APP_NAME=Laravel
APP_ENV=local
APP_KEY= your_app_key
APP_DEBUG=true
APP_TIMEZONE=UTC
APP_URL=http://localhost


# Bahasa & Lokalisasi
APP_LOCALE=en
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=en_US


# Mode Maintenance
APP_MAINTENANCE_DRIVER=file
APP_MAINTENANCE_STORE=database


# Keamanan Password
BCRYPT_ROUNDS=12


# Log
LOG_CHANNEL=stack
LOG_STACK=single
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug


# Database (default: PostgreSQL)
# Isi sesuai database kamu.
DB_CONNECTION=pgsql
DB_HOST=127.0.0.1
DB_PORT=5432
DB_DATABASE=algorify
DB_USERNAME=postgres
DB_PASSWORD=

# Contoh untuk Postgres managed (Supabase/Neon/dll)
# Catatan: jangan taruh data asli di file ini; simpan di file .env lokal kamu.
# DB_CONNECTION=pgsql
# DB_HOST=your-managed-postgres-host
# DB_PORT=5432
# DB_DATABASE=your-database
# DB_USERNAME=your-username
# DB_PASSWORD=your-password


# Session
# SESSION_DRIVER=database butuh tabel session (jalankan migrate)
SESSION_DRIVER=database
SESSION_LIFETIME=120
SESSION_ENCRYPT=false
SESSION_PATH=/
SESSION_DOMAIN=null


# Layanan
# QUEUE_CONNECTION=database menyimpan job di database (jalankan migrate)
BROADCAST_CONNECTION=log
FILESYSTEM_DISK=local
QUEUE_CONNECTION=database


# Cache
# CACHE_STORE=database menyimpan cache di database (jalankan migrate)
CACHE_STORE=database
CACHE_PREFIX=


# Memcached (opsional)
MEMCACHED_HOST=127.0.0.1


# Redis (opsional)
REDIS_CLIENT=phpredis
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379


# Email
# MAIL_MAILER=log artinya email hanya dicatat ke log (tidak benar-benar terkirim)
# Untuk SMTP: set MAIL_MAILER=smtp lalu isi host/port/username/password
MAIL_MAILER=log
MAIL_HOST=127.0.0.1
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_ENCRYPTION=null
MAIL_FROM_ADDRESS="hello@example.com"
MAIL_FROM_NAME="${APP_NAME}"


# AWS / S3 (opsional)
# Dipakai kalau FILESYSTEM_DISK=s3
AWS_ACCESS_KEY_ID= your_aws_access_key_id
AWS_SECRET_ACCESS_KEY= your_aws_secret_access_key
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET= your_aws_bucket
AWS_USE_PATH_STYLE_ENDPOINT=false


# Vite
# Variabel yang diawali VITE_ bisa terbaca di frontend.
VITE_APP_NAME="${APP_NAME}"


# Google OAuth (opsional)
# Isi ini kalau kamu mengaktifkan login Google.
# GOOGLE_REDIRECT_URL harus sama persis dengan Redirect URI di Google Cloud Console.
GOOGLE_CLIENT_ID= your_google_client_id
GOOGLE_CLIENT_SECRET= your_google_client_secret
GOOGLE_REDIRECT_URL=http://localhost:8000/auth/google/callback


# Verifikasi SSL untuk Socialite
# Kalau di Windows SSL sering error, kamu bisa set sementara jadi false.
# Untuk production, sebaiknya true.

SOCIALITE_HTTP_VERIFY=true
# Opsional: isi path CA bundle di Windows (contoh: C:\\certs\\cacert.pem)
# SOCIALITE_CA_PATH=


# Konfigurasi Payment DOKU (opsional)
# Biarkan kosong kalau tidak memakai integrasi DOKU.
# DOKU_DISABLE_SSL_VERIFY: untuk production sebaiknya false
DOKU_CLIENT_ID= your_doku_client_id
DOKU_SECRET_KEY= your_doku_secret_key
DOKU_IS_PRODUCTION=false
DOKU_DISABLE_SSL_VERIFY=true